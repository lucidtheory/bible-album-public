{
  "name": "bible-memory-book",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "lint": "eslint --ext .ts,.tsx",
    "clean": "watchman watch-del-all && rm -rf /tmp/haste-map-react-native-packager-* && yarn cache clean && expo prebuild --clean",
    "publish:prod": "ENV=production eas update --release-channel production",
    "publish:staging": "ENV=staging eas update --release-channel staging",
    "build:prod": "ENV=production eas build --profile production --auto-submit",
    "build:staging": "ENV=staging eas build --profile staging --auto-submit"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*|@react-native/.*|@expo-google-fonts/.*|@react-native-firebase/.*)"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ]
  },
  "dependencies": {
    "@amplitude/react-native": "^2.16.1",
    "@expo-google-fonts/cinzel": "^0.2.2",
    "@expo-google-fonts/cinzel-decorative": "^0.2.2",
    "@expo-google-fonts/josefin-sans": "^0.2.2",
    "@expo/vector-icons": "^13.0.0",
    "@react-native-async-storage/async-storage": "~1.17.3",
    "@react-native-clipboard/clipboard": "^1.11.1",
    "@react-native-community/netinfo": "9.3.5",
    "@react-native-community/progress-view": "^1.3.2",
    "@react-native-community/slider": "4.2.4",
    "@react-native-firebase/app": "^16.3.1",
    "@react-native-firebase/app-check": "^16.3.1",
    "@react-native-firebase/crashlytics": "^16.3.1",
    "@react-native-firebase/perf": "^16.3.1",
    "@react-navigation/bottom-tabs": "^6.4.0",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.1",
    "@react-navigation/stack": "^6.3.2",
    "@reduxjs/toolkit": "^1.8.6",
    "@shopify/flash-list": "1.3.1",
    "axios": "0.27.1",
    "expo": "^47.0.0",
    "expo-build-properties": "~0.4.1",
    "expo-constants": "~14.0.2",
    "expo-dev-client": "~2.0.1",
    "expo-font": "~11.0.1",
    "expo-navigation-bar": "~2.0.1",
    "expo-splash-screen": "~0.17.5",
    "expo-status-bar": "~1.4.2",
    "expo-updates": "~0.15.6",
    "hex-rgb": "^5.0.0",
    "jest": "^26.6.3",
    "lottie-react-native": "5.1.4",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-native": "0.70.5",
    "react-native-blob-util": "^0.16.3",
    "react-native-fast-image": "^8.6.1",
    "react-native-in-app-review": "^4.1.1",
    "react-native-pdf": "^6.6.2",
    "react-native-safe-area-context": "4.4.1",
    "react-native-screens": "~3.18.0",
    "react-native-web": "~0.18.7",
    "react-redux": "^8.0.4",
